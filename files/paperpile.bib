@ARTICLE{Stevenson2018-fm,
  title    = "{Assessing risk assessment in action}",
  author   = "Stevenson, Megan",
  journal  = "Minnesota law review",
  volume   =  103,
  number   =  1,
  pages    = "303--384",
  abstract = "Recent years have seen a rush towards evidence-based tools in
              criminal justice. As part of this movement, many jurisdictions
